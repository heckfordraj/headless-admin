type Page {
  id: String;
  title: String;
  data: String;
  validate() { this.id == key() }
}

path /pages {
  read() { true }

  /{page} is Page {
      create() { prior(this) == null }
      update() { prior(this) == null }
      delete() { true }
  }
}

path /data {
  read() { true }
  write() { true }
}
